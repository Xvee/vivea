# -*- coding: utf-8 -*-
import random
in_num = int(input("输入石头(1)剪刀(2)布(3)对应的数字：\n"))
computer_num = random.randint (1,3)
num_dict1 = {1:"石头",2:"剪刀",3:"布"}
num_dict2 = {(1,1):"平局",(1,2):"你赢了",(1,3):"你输了",(2,1):"你输了",(2,2):"平局",(2,3):"你赢了",(3,1):"你赢了",(3,2):"你输了",(3,3):"平局"}
print("你出了 %s，计算机出了 %s"%(num_dict1[in_num],num_dict1[computer_num]))
print(num_dict2[in_num,computer_num])
